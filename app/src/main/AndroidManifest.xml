<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.am.widget">

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.READ_PHONE_STATE" />

    <application
        android:allowBackup="true"
        android:icon="@mipmap/ic_launcher"
        android:label="@string/app_name">
        <activity android:name="com.am.activity.WidgetListActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.am.activity.NewWidgetActivity"
            android:label="@string/newwidget" />
        <activity
            android:name="com.am.activity.WechatActivity"
            android:label="@string/wechat" />
        <activity
            android:name="com.am.activity.BilateralPaneActivity"
            android:label="@string/bilateralpane" />
        <activity
            android:name="com.am.activity.DotpagerActivity"
            android:label="@string/dotpager" />
        <activity
            android:name="com.am.activity.SuperTabActivity"
            android:label="@string/supertab">
            <intent-filter>
                <action android:name="android.intent.action.VIEW" />

                <category android:name="android.intent.category.DEFAULT" />
                <category android:name="android.intent.category.BROWSABLE" />

                <data
                    android:host="github.com"
                    android:pathPrefix="/AlexMofer/GradientPagerTabStrip"
                    android:scheme="https" />
            </intent-filter>
        </activity>
        <activity
            android:name="com.am.activity.SwipeRefreshActivity"
            android:label="@string/swiperefresh" />
        <activity
            android:name="com.am.activity.DragGridViewActivity"
            android:label="@string/draggridview" />
        <activity
            android:name="com.am.activity.ShapeImageViewActivity"
            android:label="@string/shapeimage" />
        <activity
            android:name="com.am.activity.CityListActivity"
            android:label="@string/citylist" />
        <activity
            android:name="com.am.activity.SharpCornerBoxActivity"
            android:label="@string/sharpcornerbox" />
        <activity
            android:name="com.am.activity.ConnectImageActivity"
            android:label="@string/connectimage" />
        <activity
            android:name="com.am.activity.StateFrameActivity"
            android:label="@string/stateframe" />
        <activity
            android:name="com.am.activity.ReplaceActivity"
            android:label="@string/replace" />
        <activity
            android:name="com.am.activity.DrawableRatingBarActivity"
            android:label="@string/drawableratingbar" />
        <activity
            android:name="com.am.activity.WrapActivity"
            android:label="@string/wrap" />
        <activity
            android:name="com.am.activity.PrinterActivity"
            android:label="@string/printer" />
        <activity
            android:name="com.am.activity.SecurityCodeActivity"
            android:label="@string/security_code" />
        <activity
            android:name="com.am.activity.LoadingActivity"
            android:label="@string/loading_drawable" />
        <activity
            android:name="com.am.activity.SecurityActivity"
            android:label="@string/security_check" />
        <activity
            android:name="com.am.activity.CipherActivity"
            android:label="@string/cipher" />
    </application>

</manifest>